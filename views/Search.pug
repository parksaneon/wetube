extends layouts/main
include mixins/videoBlock

block content
    .search_header
        h3 Searching for #{searchingBy}
    .search_videos
        if videos.length === 0
            h3 No Videos Found
        each video in videos
            +videoBlock({
                title:video.title,
                views:video.views,
                videoFile:video.videoFile,
                id:video.id
            })